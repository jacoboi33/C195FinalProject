                  </VBox>
                  <VBox alignment="CENTER_RIGHT" prefHeight="200.0" prefWidth="100.0">
                     <HBox.margin>
                        <Insets right="25.0" />
                     </HBox.margin>
                     <children>
                        <Button fx:id="cancelButton" maxWidth="-Infinity" mnemonicParsing="false" onAction="#onActionViewAppointments" prefHeight="27.0" prefWidth="75.0" style="-fx-background-color: #03a1fc;" text="CANCEL" />
                     </children>
                  </VBox>
               </children>
